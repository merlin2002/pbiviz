{
    "dataRoles": [
        {
            "displayName": "类别",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "值",
            "name": "measure",
            "kind": "Measure"
        },
        {
            "displayName": "图列",
            "name": "series",
            "kind": "Grouping"
        }
    ],
    "objects": {
        "dataPoint": {
            "displayName": "Data colors",
            "properties": {
                "defaultColor": {
                    "displayName": "Default color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "theme": {
                    "displayName": "主题样式",
                    "type": {
                        "enumeration": [
                            {
                                "displayName": "Light",
                                "value": "light"
                            },
                            {
                                "displayName": "Dark",
                                "value": "dark"
                            }
                        ]
                    }
                },
                "showAllDataPoints": {
                    "displayName": "Show all",
                    "type": {
                        "bool": true
                    }
                },
                "fill": {
                    "displayName": "Fill",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fillRule": {
                    "displayName": "Color saturation",
                    "type": {
                        "fill": {}
                    }
                },
                "fontSize": {
                    "displayName": "Text Size",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    }
                },
                "values": {
                    "group": {
                        "by": "series",
                        "select": [
                            {
                                "for": {
                                    "in": "measure"
                                }
                            }
                        ]
                    }
                }
            }
        }
    ],
    "sorting": {
        "default": {   }
    }
}