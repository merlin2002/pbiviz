{
    "dataRoles": [
        {
            "displayName": "类别",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "值",
            "name": "measure",
            "kind": "Measure"
        },
        {
            "displayName": "图列",
            "name": "series",
            "kind": "Grouping"
        }
    ],
    "drilldown": {
        "roles": [
            "category"
        ]
    },
    "objects": {
        "myproperties": {
            "displayName": "自定义属性",
            "properties": {
                "theme": {
                    "displayName": "主题颜色",
                    "type": {
                        "enumeration": [
                            {
                                "displayName": "Default",
                                "value": "default"
                            },
                            {
                                "displayName": "Infographic",
                                "value": "infographic"
                            },
                            {
                                "displayName": "Chalk",
                                "value": "chalk"
                            },
                            {
                                "displayName": "Vintage",
                                "value": "vintage"
                            },
                            {
                                "displayName": "Dark",
                                "value": "dark"
                            },
                            {
                                "displayName": "Walden",
                                "value": "walden"
                            },
                            {
                                "displayName": "Roma",
                                "value": "roma"
                            },
                            {
                                "displayName": "Shine",
                                "value": "shine"
                            }
                        ]
                    }
                },
                "showtoolbox": {
                    "displayName": "显示工具条",
                    "type": {
                        "bool": true
                    }
                },
                "showlegend": {
                    "displayName": "显示图例",
                    "type": {
                        "bool": true
                    }
                },
                "renderer": {
                    "displayName": "渲染方式",
                    "type": {
                        "enumeration": [
                            {
                                "displayName": "Canvas",
                                "value": "canvas"
                            },
                            {
                                "displayName": "SVG",
                                "value": "svg"
                            }
                        ]
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 2
                    }
                },
                {
                    "measure": {
                        "max": 1
                    }
                },
                {
                    "series": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    }
                },
                "values": {
                    "group": {
                        "by": "series",
                        "select": [
                            {
                                "for": {
                                    "in": "measure"
                                }
                            }
                        ]
                    }
                }
            }
        }
    ],
    "sorting": {
        "default": {}
    }
}